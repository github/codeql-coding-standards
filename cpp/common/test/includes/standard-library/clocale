#ifndef _GHLIBCPP_CLOCALE
#define _GHLIBCPP_CLOCALE

#define NULL 0
#define LC_ALL 0
#define LC_COLLATE 0
#define LC_CTYPE 0
#define LC_MONETARY 0
#define LC_NUMERIC 0
#define LC_TIME 0

#include <locale.h>

namespace std {
using ::lconv;
using ::localeconv;
using ::setlocale;
} // namespace std

#endif // _GHLIBCPP_CLOCALE